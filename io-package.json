{
    "common": {
        "name": "system-health",
        "version": "0.1.0",
        "title": "System Health & State Inspector",
        "titleLang": {
            "en": "System Health & State Inspector",
            "de": "Systemgesundheit & Datenpunkt-Inspektor"
        },
        "desc": {
            "en": "Monitor ioBroker system health and inspect state quality",
            "de": "Überwacht die Systemgesundheit von ioBroker und prüft die Datenpunkt-Qualität"
        },
        "authors": [
            "ioBroker Community AI Agents"
        ],
        "license": "MIT",
        "platform": "Javascript/Node.js",
        "mode": "daemon",
        "compact": true,
        "connectionType": "local",
        "dataSource": "push",
        "type": "utility",
        "tier": 3,
        "keywords": [
            "system-health",
            "monitoring",
            "diagnostics",
            "inspector",
            "cleanup"
        ],
        "readme": "https://github.com/Skeletor-ai/ioBroker.system-health/blob/main/README.md",
        "icon": "system-health.svg",
        "extIcon": "https://raw.githubusercontent.com/Skeletor-ai/ioBroker.system-health/main/admin/system-health.svg",
        "enabled": true,
        "messagebox": true,
        "dependencies": [
            {
                "js-controller": ">=5.0.0"
            }
        ],
        "adminUI": {
            "config": "json"
        },
        "adminTab": {
            "singleton": true,
            "name": {
                "en": "System Health",
                "de": "Systemgesundheit"
            },
            "fa-icon": "fa-heartbeat",
            "link": "jsonTab.json5"
        }
    },
    "native": {
        "checkIntervalHours": 6,
        "healthCheckIntervalMinutes": 5,
        "memoryWarningMB": 500,
        "diskWarningPercent": 90,
        "staleThresholdHours": 24,
        "stateInspectorIgnorePatterns": [],
        "enableOrphanDetection": true,
        "enableDuplicateDetection": true,
        "duplicateSimilarityThreshold": 0.9,
        "enableStaleDetection": true,
        "enableAdapterCrashDetection": true,
        "enableMemoryMonitoring": true,
        "enableDiskMonitoring": true,
        "enablePerformanceAnalysis": false,
        "performanceUpdateFrequencyMs": 60000
    },
    "objects": [],
    "instanceObjects": []
}
